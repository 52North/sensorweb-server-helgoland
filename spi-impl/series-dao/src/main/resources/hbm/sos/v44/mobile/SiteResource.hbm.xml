<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="org.n52.series.db.da.beans.v2">
  <class name="SiteEntity" table="featureOfInterest">
    <id name="pkid" type="long">
      <column name="featureofinterestid"/>
      <generator class="assigned"/>
    </id>
    <property column="identifier" insert="false" name="domainId" type="string" update="false"/>
    <property column="name" name="name" type="string"/>
    <property column="description" name="name" type="string" insert="false" update="false"/>
    <property column="geom" name="geom" type="org.hibernate.spatial.GeometryType"/>
  </class>
</hibernate-mapping>
